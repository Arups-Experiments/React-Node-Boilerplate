{
  "parser": "@babel/eslint-parser",
  "ignorePatterns": ["node_modules/", "*.test.js"],
  "plugins": ["prettier", "import", "react", "prefer-object-spread"],
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "env": {
    "browser": true,
    "commonjs": true,
    "node": true,
    "es6": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "rules": {}
}
